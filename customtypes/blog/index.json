{
  "format": "custom",
  "id": "blog",
  "label": "blog",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "title",
          "placeholder": ""
        }
      },
      "excerpt": {
        "type": "Text",
        "config": {
          "label": "excerpt",
          "placeholder": ""
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "date": {
        "type": "Timestamp",
        "config": {
          "label": "date",
          "placeholder": ""
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "content",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "authors": {
        "type": "Link",
        "config": {
          "label": "authors",
          "select": "document",
          "repeat": false,
          "customtypes": [
            {
              "id": "author",
              "fields": ["name", "avatar"]
            }
          ]
        }
      },
      "category": {
        "type": "Link",
        "config": {
          "label": "category",
          "select": "document",
          "repeat": false,
          "customtypes": [
            {
              "id": "categories",
              "fields": ["name"]
            }
          ]
        }
      }
    }
  }
}
